    "nums = [2, 7, 11, 15]\n",
    "\n",
    "def findtwo(nums,target):\n",
    "    dic = {}\n",
    "    for i in range(len(nums)):\n",
    "        if nums[i] in dic:\n",
    "            return [dic[nums[i]],i]\n",
    "        dic[target -nums[i]] = i\n",
    "    return None\n",
    "\n",
    "findtwo(nums, 9)"
   ]
