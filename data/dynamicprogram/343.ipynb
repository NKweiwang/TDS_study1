    "n = 10\n",
    "\n",
    "def mprod(n):\n",
    "    k3 = n//3\n",
    "    m3 = n %3\n",
    "    if m3 == 0:\n",
    "        return 3**k3\n",
    "    elif m3 ==2:\n",
    "        return 3** k3 *m3\n",
    "    else:\n",
    "        return 3**(k3-1) * 4\n",
    "mprod(3)"
   ]
