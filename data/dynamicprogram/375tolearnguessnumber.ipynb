   "source": [
    "n = 10\n",
    "crt = n\n",
    "k = 0\n",
    "while crt > 0:\n",
    "    k += 1\n",
    "    crt = crt //2\n",
    "crtsum = 0\n",
    "\n",
    "for k in range(1,k+1):\n",
    "    crtsum += (1-2**(-k))\n",
    "int(n * crtsum) + 1"
   ]
